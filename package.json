{
  "name": "react-router-transition-switch",
  "version": "0.0.0-development",
  "description": "a <Switch> variant that's easier to use with transition components",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src test stories",
    "lint:fix": "eslint --fix src test stories",
    "lint:watch": "esw --watch src test stories",
    "flow": "flow",
    "flow:coverage": "for file in src/**.js test/**.js; do echo $file; flow coverage $file; done",
    "flow:watch": "flow-watch -e js,js.flow,flowconfig --ignore lib/ --ignore node_modules/ --watch .flowconfig --watch src/ --watch test/",
    "gen-flow-files": "flow gen-flow-files src/ --out-dir lib",
    "copy-flow-files": "cd src; copy *.js.flow **/*.js.flow ../lib",
    "build": "rimraf lib && babel src --out-dir lib",
    "test": "NODE_ENV=test karma start --single-run",
    "test:watch": "NODE_ENV=test karma start",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run lint && flow",
    "prepush": "npm test",
    "prepublish": "npm run lint && flow && npm test && npm run build && npm run copy-flow-files",
    "open:coverage": "open coverage/lcov-report/index.html",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "config": {
    "mocha": "--compilers js:babel-core/register -r jsdom-global/register ./test/**.js",
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcoreio/react-router-better-switch.git"
  },
  "keywords": [
    "react-router",
    "transitions",
    "animation"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcoreio/react-router-better-switch/issues"
  },
  "homepage": "https://github.com/jcoreio/react-router-better-switch#readme",
  "devDependencies": {
    "@jedwards1211/eslint-config-flow": "^1.0.0",
    "@jedwards1211/eslint-config-react": "^2.0.1",
    "@storybook/react": "^3.1.6",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-flow-react-proptypes": "^9.1.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "chai": "^3.5.0",
    "copy": "^0.3.0",
    "coveralls": "^2.11.16",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^3.13.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-watch": "^3.0.0",
    "flow-bin": "^0.56.0",
    "flow-watch": "^1.1.0",
    "history": "^4.7.2",
    "husky": "^0.13.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-es6-shim": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "phantomjs-prebuilt": "^2.1.16",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.5.4",
    "semantic-release": "^7.0.2",
    "validate-commit-msg": "^2.8.2"
  },
  "peerDependencies": {
    "react": "0.14.x || ^15.0.0 || ^16",
    "react-router": "^4.2.0"
  },
  "dependencies": {
    "invariant": "^2.2.2",
    "prop-types": "^15.6.0",
    "sinon": "^3.2.1",
    "warning": "^3.0.0"
  }
}
